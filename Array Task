5)Move all negative elements to one side of the array?[-1,6,8,7,-4,-3] [7,8,6,-4,-3,-1]

class Ques5
{
static void elements(int arr[],int n)
{
int temp[]=new int[n];
int j=0;

for(int i=0;i<n;i++)
if(arr[i]>=0)
temp[j++]=arr[i];
if(j==n||j==0)
return;

for(int i=0;i<n;i++)
if(arr[i]<0)
temp[j++]=arr[i];

for(int i=0;i<n;i++)
arr[i]=temp[i];
}

public static void main(String[] args)
{
int arr[]={-1,6,8,7,-4,-3};
int n=arr.length;
elements(arr,n);

for(int i=0;i<n;i++)

System.out.print(arr[i]+" ");
}
}

